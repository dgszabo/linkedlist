{
    "$id": "http://example.com/example.json",
    "type": "object",
    "properties": {
        "name": {
            "$id": "/properties/name",
            "type": "string",
            "title": "The Name Schema ",
            "minLength": 1,
            "maxLength": 55
        },
        "email": {
            "$id": "/properties/email",
            "type": "string",
            "title": "The Email Schema ",
            "minLength": 1,
            "maxLength": 55,
            "pattern": "^([a-zA-Z0-9_\\\\-\\\\.]+)@([a-zA-Z0-9_\\\\-\\\\.]+)\\\\.([a-zA-Z]{2,5})$"
        },
        "handle": {
            "$id": "/properties/handle",
            "type": "string",
            "title": "The Handle Schema ",
            "minLength": 1,
            "maxLength": 55
        },
        "password": {
            "$id": "/properties/password",
            "type": "string",
            "title": "The Password Schema ",
            "minLength": 1,
            "maxLength": 55
        },
        "required": [
            "name",
            "email",
            "handle",
            "password"
        ]
    }
}